<h1>ユーザー</h1>
<% if current_user %>
    <%= link_to 'ログアウト', top_logout_path %><br>
<% else %>
    <%= link_to 'ログイン', top_main_path %><br>
<% end %>
<% @users.each do |e| %>
    <%= e.uid %>
    <%= link_to '削除', user_path(e), data: { turbo_method: :delete } %>
    <br>
<% end %>

<%= link_to "登録画面", new_user_path() %><br>
